    "maintainer": false
  }, {
    "name": "Earle Philhower",
    "url": "https://github.com/earlephilhower",
    "maintainer": true
  }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/earlephilhower/arduino-pico"
  },
  "homepage": "https://github.com/earlephilhower/arduino-pico",
  "frameworks": ["arduino"],
  "platforms": "raspberrypi",
  "build": {
    "srcDir": "src",
    "includeDir": "src"
  },
  "dependencies": {
    "RP2040WiFiNINA": "1.9.0-1",
    "FilesystemTask": "1.0.0",
    "PicoLog": "1.0.0"
  }
